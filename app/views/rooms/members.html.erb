<%= provide(:title, "Members of #{@room.name}") %>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <h1>Members of <%= @room.name %></h1>
    <div id="users">
      <% @users.each do |user| %>
        <div class="user">
          <h4><%= user.name %></h4>
        </div>
      <% end %>
    </div>
  </div>
</div>
