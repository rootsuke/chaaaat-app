<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <div id="profile">
      <div class="picture">
        <%= image_tag @user.picture.url, class: "radius" %>
      </div>
      <h1><%= @user.name %></h1>
      <% if current_user.id == @user.id %>
        <%= link_to "settings", edit_user_registration_path, class: "settings" %>
      <% end %>
    </div>
  </div>
</div>
